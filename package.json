{
  "name": "react-i13n-ga",
  "description": "google analytics plugin for react i13n",
  "version": "0.1.0",
  "browser": "index.client.js",
  "main": "index.server.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:kaesonho/react-i13n-ga.git"
  },
  "scripts": {
    "cover": "istanbul cover --dir ${COVERAGE_DIR:-artifacts} -- _mocha tests/unit --recursive --reporter tap",
    "lint": "jshint",
    "test": "mocha tests/unit --recursive --reporter spec"
  },
  "devDependencies": {
    "chai": "^2.1.2",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.1",
    "mocha": "^2.0"
  },
  "keywords": [
    "react",
    "component",
    "reactors"
  ],
  "precommit": [
    "lint",
    "test"
  ],
  "yhint": {
    "excludes": [
      "/dist/",
      "/tests/"
    ]
  },
  "jshintConfig": {
    "node": true
  }
}
